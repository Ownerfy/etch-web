{
  "hosting": {
    "site": "etch-web-dev",
    "public": "dist",
    "headers": [
      {
        "source": "**",
        "headers": [
          {"key": "Access-Control-Allow-Origin", "value": "*"},
          {"key": "X-Frame-Options", "value": "deny"},
          {"key": "X-Content-Type-Options", "value": "nosniff"},
          {"key": "X-XSS-Protection", "value": "1; mode=block"}
        ]
      }
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "database": {
      "port": 9000
    },
    "hosting": {
      "port": 5000
    },
    "pubsub": {
      "port": 8085
    },
    "ui": {
      "enabled": true
    },
    "auth": {
      "port": 9099
    },
    "singleProjectMode": true
  }
}
